{
  "version": 8,
  "name": "tilejson",
  "center": [0, 0],
  "zoom": 2,
  "sources": {
    "tilejson": {
      "type": "raster",
      "url": "https://api.tiles.mapbox.com/v3/mapbox.geography-class.json?secure"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "rgba(0,0,0,0)"
      }
    },
    {
      "id": "tilejson-layer",
      "type": "raster",
      "source": "tilejson"
    }
  ]
}
